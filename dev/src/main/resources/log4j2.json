{
  "configuration":{
    "name":"Default",
    "appenders":{
      "Console":{
        "name":"console",
        "target":"SYSTEM_OUT",
        "PatternLayout":{
          "pattern":"%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"
        }
      }
    },
    "loggers":{
      "root":{
        "level":"error",
        "appender-ref":{"ref":"console", "level":"error"}
      },
      "logger":{
        "name":"ch.unibas.dmi.dbis.reqman.templating",
        "level":"debug",
        "additivity":false,
        "appender-ref":{"ref":"console"}
      }
    }
  }

}